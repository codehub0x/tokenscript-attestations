AttestationRequest

DEFINITIONS ::=
BEGIN

IMPORTS
    SubjectPublicKeyInfo
        FROM AttestationFramework
    Proof
        FROM ProofOfExponent;

Identity ::= SEQUENCE {
    identityPayload    IdentityPayload,
    publicKey          SubjectPublicKeyInfo,
}

IdentityPayload ::= SEQUENCE {
    type               IdentifierType,
    proof              Proof
}
		
IdentifierType ::= INTEGER { email(0), phone(1)}

END